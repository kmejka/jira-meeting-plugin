{
  "name": "Jira Meeting plugin",
  "description": "Jira plugin to schedule meetings",
  "key": "com.spartez.jira-meeting",
  "baseUrl": "https://0b545224.ngrok.io",
  "icon": "/favicon.ico",
  "vendor": {
    "name": "Michu & Krzychu",
    "url": "https://spartez.com/",
    "icon": "/favicon.ico"
  },
  "authentication": {
    "type": "none"
  },
  "scopes": [
    "read",
    "write",
    "admin"
  ],
  "apiVersion": 1,
  "modules": {
    "dialogs": [
      {
        "url": "/meeting-dialog.html?issueKey={issue.key}",
        "options": {
          "size": "medium",
          "header": {
            "value": "Choose meeting date"
          }
        },
        "key": "meeting-dialog"
      }
    ],
    "webItems": [
      {
        "location": "jira.issue.tools",
        "context": "addon",
        "weight": 200,
        "target": {
          "type": "dialogmodule",
          "options": {
            "key": "meeting-dialog"
          }
        },
        "key": "meeting-button",
        "name": {
          "value": "Schedule a meeting"
        }
      }
    ],
    "jiraIssueFields": [
      {
        "key": "meeting-date-field",
        "name": {
          "value": "Meeting date"
        },
        "description": {
          "value": "Meeting start date"
        },
        "type": "string"
      },
      {
        "key": "meeting-hangout-link",
        "name": {
          "value": "Meeting hangout link"
        },
        "description": {
          "value": "Hangout link"
        },
        "type": "string"
      }
    ]
  }
}
